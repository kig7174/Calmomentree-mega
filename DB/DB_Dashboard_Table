CREATE TABLE IF NOT EXISTS `calmomentree`.`sales` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `date` DATE NOT NULL COMMENT '날짜',
  `total_sales` INT NOT NULL COMMENT '일 매출액',
  PRIMARY KEY (`id`))
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8
COLLATE = utf8_bin


CREATE TABLE IF NOT EXISTS `calmomentree`.`new_member` (
  `id` INT NOT NULL AUTO_INCREMENT,
  `date` DATE NOT NULL COMMENT '날짜',
  `member_count` INT NOT NULL COMMENT '신규 회원 수',
  PRIMARY KEY (`id`))
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8
COLLATE = utf8_bin


